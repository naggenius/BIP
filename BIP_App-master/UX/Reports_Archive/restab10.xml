    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
l_codsg varchar2(10);
begin
  if :p_param6 = 'SG..' then 
	:p_nometat := 'RCB1';
	:p_where := 'and SR.soccode like ''SG..%''';
  elsif :p_param6 = 'SSII' then
	:p_nometat := 'RCB3';
	:p_where := 'and SR.soccode != ''SG..''';
  end if;
 

 if (:P_PARAM5!=0) and (:P_PARAM5 is not null) and (:P_PARAM7='ACH') then
	:p_where := :p_where ||' and to_char(S.scentrefrais)= '''||:P_PARAM5||''' ';
 else
  if (:p_param2<>'000000') and (:P_PARAM7!='ACH' or :P_PARAM7 IS NULL ) then

	if substr(:p_param2,3,2)='00' then --dept
		l_codsg := substr(:p_param2,1,2) || '%';
	else
		if substr(:p_param2,5,2)='00' then --pole
			l_codsg := substr(:p_param2,1,4) || '%';
		else
			l_codsg :=:p_param2 || '%';
		end if;

  	end if;
  	:p_where := :p_where ||' and to_char(S.codsg,''FM000000'') like '''|| l_codsg ||''' ';
	:P_PARAM5 := NULL;
  end if;

 end if;
  --srw.message(1,:p_where);
  return (TRUE);
end;]]>
      </textSource>
    </function>
