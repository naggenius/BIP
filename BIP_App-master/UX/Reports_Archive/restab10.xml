    <function name="f_libdsgformattrigger">
      <textSource>
      <![CDATA[function F_libdsgFormatTrigger return boolean is
Text varchar2(256);
begin
  if   ( SUBSTR(:P_PARAM2,3,2)= '00' or  SUBSTR(:P_PARAM2,5,2)= '00' or :P_PARAM7='ACHMENU') then 
	
	 Text := :libdir ||'/'|| :sigdep ||'/'|| :sigpole;
	 SRW.SET_FIELD_CHAR(0,Text);
  end if;
  return (TRUE);
end;]]>
