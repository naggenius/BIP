    <function name="afterpform">
      <textSource>
      <![CDATA[------------------------------------------------------------------------------------------
-- Rôle : Construire un Where conditionnel. La chaine construite sera affectée au parametre
-- de P_WHERE. 
------------------------------------------------------------------------------------------
function AfterPForm return boolean is
	
	l_perim_me	VARCHAR2(1000) := '';
  p_dpg VARCHAR2(10) := '';
  
begin
  :p_where := '';
  
-- Dates d'enregistrement comptable

  IF :P_PARAM7 IS NOT NULL THEN 
		  	--srw.message(1,:P_PARAM7);
				:p_where := :P_where || ' and F.FENRCOMPTA >= TO_DATE(''' || :P_PARAM7 || ''',''dd/mm/yyyy'')';
  END IF;

  IF :P_PARAM8 IS NOT NULL THEN
  	    --srw.message(2,:P_PARAM8);
        :p_where := :P_where || ' and F.FENRCOMPTA <= TO_DATE(''' || :P_PARAM8 || ''',''dd/mm/yyyy'')';
  END IF;


-- Dates de la prestation

  IF :P_PARAM9 IS NOT NULL THEN 
  			--srw.message(3,:P_PARAM9);
				:p_where := :P_where || ' and L.LMOISPREST >= TO_DATE(''' || :P_PARAM9  || ''',''dd/mm/yyyy'')';
  END IF;

  IF :P_PARAM10 IS NOT NULL THEN 
  			--srw.message(4,:P_PARAM10);
        :p_where := :P_where || ' and L.LMOISPREST <= TO_DATE(''' || :P_PARAM10 || ''',''dd/mm/yyyy'')';
  END IF;


-- DPG du contrat

	l_perim_me := pack_global.lire_perime(:P_GLOBAL);
	
  IF :P_PARAM11 IS NOT NULL THEN 
  
  		p_dpg := RTRIM(LTRIM(:P_PARAM11));	
  		p_dpg := REPLACE(p_dpg,'*','%');
  				
			:p_where :=  :p_where || ' AND C.codsg IN (SELECT codsg FROM vue_dpg_perime where INSTR('''|| l_perim_me ||''', codbddpg) > 0 )';

			:p_where := :p_where || ' and  to_char(C.codsg,''FM0000000'') like ''' || p_dpg || '''';
	
  ELSE
  		:p_where :=  :p_where || ' AND C.codsg IN (SELECT codsg FROM vue_dpg_perime where INSTR('''|| l_perim_me ||''', codbddpg) > 0 )';
  END IF;

  :p_where := :p_where || ' ';

  --srw.message(6,:p_where);

  RETURN (TRUE);
END;

]]>
      </textSource>
    </function>
    <function name="rtrimetoile">
      <textSource>
      <![CDATA[FUNCTION RtrimEtoile(P_Text In VARCHAR2) RETURN VARCHAR2 IS
  meta VARCHAR2(1);
  L_Text VARCHAR2(7);
BEGIN
  L_Text := P_Text;
  meta := SUBSTR(P_Text, NVL(length(P_Text), 0),1);
  IF (meta = ' ') OR (meta = '*') THEN
      L_Text := RTRIM(P_Text,meta);   
  END IF;
  RETURN(L_Text);
END;]]>
      </textSource>
    </function>
