    <function name="afterpform">
      <textSource>
      <![CDATA[------------------------------------------------------------------------------------------
-- Rôle : Construire un Where conditionnel. La chaine construite sera affectée au parametre
-- de P_WHERE. 
------------------------------------------------------------------------------------------
function AfterPForm return boolean is
begin
  -- p_where1 sera pour selectionner les factures avec contrat et
  -- p_where2 pour les factures sans contrat.

  :p_where1 := '';
  IF :P_PARAM6 IS NOT NULL THEN 
	:p_where1 := :p_where1 || ' and f.socfact = ''' || :P_PARAM6 ||'''';
  END IF;
    
  IF (:P_PARAM7 IS NOT NULL) and (:P_PARAM8 IS NOT NULL) THEN 
	:p_where1 := :P_where1 || ' and f.FREGCOMPTA >= TO_DATE(''' || :P_PARAM7 || ''',''dd/mm/yyyy'')';
        :p_where1 := :P_where1 || ' and f.FREGCOMPTA <= TO_DATE(''' || :P_PARAM8 || ''',''dd/mm/yyyy'')';

  END IF;

  IF (:P_PARAM5 !='0')  THEN 
	:P_WHERE1 := :P_WHERE1 || ' and to_char(f.FCENTREFRAIS) = ''' || :P_PARAM5 ||'''';
  END IF;

  :p_where2 := :p_where1; 

  IF :P_PARAM3 IS NOT NULL THEN 
	:p_where1 := :p_where1 || ' and c.filcode = ''' || :P_PARAM3 ||'''';
  END IF;

  :p_where1 := :p_where1 || ' ';
  :p_where2 := :p_where2 || ' ';
  --srw.message(1,:p_where1);
  --srw.message(1,:p_where2);
  RETURN (TRUE);
END;

]]>
      </textSource>
    </function>
    <function name="beforereport">
      <textSource>
      <![CDATA[function BeforeReport return boolean is
  l_Filtre varchar2(100);  -- Sert à construire les lignes de la rubrique critères de selection
  l_Text varchar2(100);
  l_Text2 varchar2(100);
begin    
  :P_filtre := ' ';
  IF :P_PARAM7 IS NOT NULL THEN 
	l_Filtre := 'SELECTION DU : ' || :P_PARAM7 || ' AU : ' || :P_PARAM8;
        l_Filtre := RPAD(l_Filtre, 45);
        :P_filtre := :P_filtre || l_Filtre;
  END IF;

  return (TRUE);
end;]]>
      </textSource>
    </function>
    <function name="f_cfrais1formattrigger">
      <textSource>
      <![CDATA[function F_CFRAIS1FormatTrigger return boolean is
Text varchar2(60);
begin
  if NVL(LENGTH(:P_PARAM5), 0)!= 0 then 
	 Select LIBCFRAIS into Text from centre_frais where to_char(codcfrais) = :P_PARAM5;
	 Text := :P_PARAM5 || ' - ' ||Text;
	 SRW.SET_FIELD_CHAR(0,Text);
  end if;
  return (TRUE);
end;]]>
      </textSource>
    </function>
