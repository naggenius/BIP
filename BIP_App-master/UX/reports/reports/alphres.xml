    <function name="type_ress" returnType="character">
      <textSource>
      <![CDATA[function type_ress return VARCHAR2 is
begin
  if :rtype = 'P' then 
	return('PERSONNE');
  elsif (:rtype = 'F' or :rtype = 'E') then
	return('FORFAIT');
  elsif :rtype = 'L' then 
	return('LOGICIEL');
	else return null;
  end if; 
end;]]>
      </textSource>
    </function>
    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
begin
  
  :p_where := 'and si.codsg = s.codsg ';
  
  
  /* TD 808 à la demande G. PAPON on supprime le filte sur le centre frais
  if (:p_param7 <> 0) then
  	:p_where := :p_where || 'and si.scentrefrais in ('|| :p_param7 ||')';
  end if;*/

  
  return (TRUE);
end;]]>
      </textSource>
    </function>
