    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
l_zero VARCHAR2(2);
L_CODSG varchar2(10);

begin
l_zero := '00';
:p_dateplancher := '01/'||to_char(add_months(sysdate,-1),'MM/YYYY');

 
:p_where := ' and 1=1 ';


  if (:P_PARAM5!=0) and (:P_PARAM5 is not null) and (:P_PARAM6='ACH') then
	:p_where := :p_where ||' and  to_char(S.scentrefrais) = ''' || :P_PARAM5 ||''' ';
  else
	if (:P_PARAM6!='ACH' or :P_PARAM6 IS NULL ) and (:P_PARAM2<>'000000') then
		if substr(:p_param2,3,2)='00' then --dept
		L_CODSG := substr(:p_param2,1,2) || '%';
	   else
		if substr(:p_param2,5,2)='00' then --pole
			L_CODSG  := substr(:p_param2,1,4) || '%';
		else
			L_CODSG  :=:p_param2 || '%';
		end if;


  	   end if;
		/*if substr(:P_PARAM2,3,2)='00' then --département
		:p_where :=:p_where || ' and ''' || :P_PARAM2 ||''' in (
				select decode(:P_PARAM2 ,0000,0000,substr(codsg,1,2)||''' || l_zero ||''')
 				from situ_ress 
				where datdep is null and ident=SR.ident ) ';

		else
	 	:p_where :=:p_where || ' and ''' || :P_PARAM2 ||''' in (
				select decode(:P_PARAM2 ,0000,0000,substr(codsg,1,4))
 				from situ_ress 
				where datdep is null and ident=SR.ident ) ';
		end if;*/

		     :p_where :=:p_where || ' and to_char(SR.codsg,''FM000000'') like '''|| L_CODSG  ||''' ';
	end if;
	
         :P_PARAM5 := NULL;
	
  end if;


--srw.message(1,:p_where);
 --srw.message(1,:p_dateplancher); 
 return (TRUE);
end;]]>
      </textSource>
    </function>
    <function name="f_cfraisformattrigger">
      <textSource>
      <![CDATA[function F_CFRAISFormatTrigger return boolean is
Text varchar2(60);
begin
  if NVL(LENGTH(:P_PARAM5), 0)!= 0 then 
	 Select LIBCFRAIS into Text from centre_frais where to_char(codcfrais) = :P_PARAM5;
	 Text := :P_PARAM5 || ' - ' ||Text;
	 SRW.SET_FIELD_CHAR(0,Text);
  end if;
  return (TRUE);
end;]]>
      </textSource>
    </function>
