    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
 l_param   VARCHAR2(40);
 l_liste   VARCHAR2(5000);
begin
 if :P_PARAM6='Toute' then
 l_liste :=pack_global.lire_globaldata(:P_GLOBAL).chefprojet;
 --srw.message(1,l_liste );  
:p_where := :p_where || ' and e.pid in (
				select pid
				from ligne_bip l, datdebex d
				where INSTR('''||l_liste||''',TO_CHAR(l.pcpi,''FM00000''))>0
				minus 
				select pid
				from ligne_bip l, datdebex d
				where INSTR('''||l_liste||''',TO_CHAR(l.pcpi,''FM00000''))>0
				and ((topfer=''O'' and TRUNC(adatestatut,''YEAR'')<TRUNC(d.datdebex,''YEAR'')  )
				or 
				(astatut  in (''A'',''D'',''C'')
	 				and TRUNC(adatestatut,''YEAR'')<TRUNC(d.datdebex,''YEAR'') )
				)
			)';

 else
 :p_where := :p_where || ' and e.pid=:P_PARAM6 ';
 end if;

   --srw.message(1,:P_where);  
  return (TRUE);
end;]]>
      </textSource>
    </function>
