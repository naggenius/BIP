    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
      l_where_dpg 	VARCHAR2(1000) := '';
      p_dpg 				VARCHAR2(10) := '';
      l_param 			VARCHAR2(10) := '';
      l_nom_colonne VARCHAR2(10) := '';
      l_perim_me		VARCHAR2(1000) := '';
      
BEGIN
	
	l_nom_colonne := 'l.CODSG';
	
	SELECT TO_NUMBER(TO_CHAR(datdebex.datdebex,'YYYY'))
	  INTO :P_ANNEE
    FROM datdebex;

	-- Filtre du perim ME
	l_perim_me := pack_global.lire_perime(:P_GLOBAL);		
	:P_WHERE :=' AND ' || l_nom_colonne || ' IN (SELECT codsg FROM vue_dpg_perime where INSTR('''|| l_perim_me ||''', codbddpg) > 0 )'; 
  
	-- On crée la where clause avec filtre sur le DPG si besoin
	-- paramètre :
	--        P_PARAM6 : variable où se trouve le DPG saisi à l'écran
	--        l_nom_colonne : nom de la colonne de la requête où l'on trouve le DPG
	
	-- On anlève tous les espaces
	p_dpg := RTRIM(LTRIM(:P_PARAM6));
	
	------------------------------------------------------------------------
 	-- Conditions WHERE sur dpg
 	-----------------------------------------------------------------------
	IF LPAD(p_dpg, 7, '*') = '*******' OR p_dpg IS NULL THEN
	  
	  l_where_dpg := '';
	  
	ELSE
		
		IF NVL(LENGTH(LPAD(p_dpg, 7, '0')), 0) != 0 then 
				-- ajout de '0' a gauche sur 7 caracteres + suppression des '*'
		  l_param := rtrim(rtrim(LPAD(p_dpg, 7, '0'),'*')) ;
			:p_where := :p_where || ' and  TO_CHAR( ' || l_nom_colonne || ' , ''FM0000000'') like ''' || l_param ||'%'' ';
		END IF;
	  
	END IF;


	if ( NVL(LENGTH(l_where_dpg), 0) != 0 ) then 
		:P_WHERE := :P_WHERE || ' AND ( ' || l_where_dpg || ' ) ';
	end if;


	
  RETURN TRUE;
end;]]>
      </textSource>
    </function>
    <function name="proposÉfournaa1formula" returnType="number">
      <textSource>
      <![CDATA[function ProposéFournAA1Formula return Number is
   p_propo1 number;
begin
  select nvl(b.BPMONTME,0) 
    into p_propo1
    from budget b 
   where :Ligne_Bip = b.pid
     and b.ANNEE = :P_ANNEE+1;
     
  return p_propo1;
exception
	when no_data_found then
	  return 0;
end;]]>
      </textSource>
    </function>
