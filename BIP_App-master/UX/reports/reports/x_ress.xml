	GROUP BY
		ident	
	) SQ_ress,
	(SELECT
		LC.ident,
		LC.numcont,
		LC.soccont,
		LC.cav,
		C.cdatdeb,
		C.cdatfin,
		LC.lcnum,
		LC.lresdeb,
		LC.lresfin
