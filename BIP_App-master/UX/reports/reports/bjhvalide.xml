      <select>
      <![CDATA[select
 -- donnees au niveau ressource/mois
	bjh_ressource.matricule
	, bjh_ressource.mois
	, bjh_ressource.ident
	, bjh_ressource.nom
	, bjh_ressource.prenom
	, bjh_ressource.dep || '/' || bjh_ressource.pole deppole
	, bjh_ressource.activite
 -- donnees au niveau anomalies
	, bjh_anomalies.typeabsence
	, bjh_anomalies.coutbip
	, bjh_anomalies.coutgip
	, NVL(bjh_anomalies.coutbip, 0) - NVL(bjh_anomalies.coutgip, 0)		difference
	, NVL(bjh_extgip.motifabs, ' ') motifabs
	, NVL(bjh_extgip.intjour, 0) intjour
	, bjh_anomalies.dateano
	, bjh_anomalies.validation1
	, bjh_anomalies.validation2
FROM bjh_ressource
	, calendrier
	, datdebex
	, bjh_anomalies
	, bjh_extgip
WHERE 	ADD_MONTHS(datdebex.datdebex, bjh_ressource.mois-1)=calendrier.calanmois
	AND bjh_ressource.matricule=bjh_anomalies.matricule(+)
	AND bjh_ressource.mois=bjh_anomalies.mois(+)
	AND bjh_anomalies.matricule=bjh_extgip.matricule(+)
	AND bjh_anomalies.mois=bjh_extgip.mois(+)
	AND bjh_anomalies.typeabsence=bjh_extgip.bipabs(+)
	AND (bjh_ressource.conso_total!=calendrier.cjours or bjh_anomalies.matricule is not null)
	-- habilitations
	AND bjh_ressource.codsg IN
		(SELECT codsg 
		FROM vue_dpg_perime
		WHERE INSTR(pack_global.lire_perime(:p_global), codbddpg) > 0
		)
	-- seulement les anomalies validees
	AND (bjh_anomalies.validation1 IS NOT NULL OR bjh_anomalies.validation2 IS NOT NULL)
&P_WHERE
ORDER BY bjh_ressource.codsg
	, bjh_ressource.matricule
	, bjh_ressource.mois
/
]]>
