    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is

begin

:p_where:='';

 
IF :P_PARAM6 IS NULL AND :P_PARAM7 IS NULL THEN
	:p_where:='';
	ELSE
	:p_where := 'AND TO_DATE(to_char(date_trait,''dd/mm/yyyy'')) >= TO_DATE(''' || :P_PARAM6 || ''',''dd/mm/yyyy'') AND TO_DATE(to_char(date_trait,''dd/mm/yyyy'')) <= TO_DATE(''' || :P_PARAM7 || ''',''dd/mm/yyyy'')';
	--srw.message(1,:p_where);
END IF;


return (true);
end;]]>
      </textSource>
    </function>
