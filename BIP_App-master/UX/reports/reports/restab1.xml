    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
begin
:p_where :='';
  if NVL(LENGTH(:P_PARAM6), 0)!= 0 then 
	if LPAD(:P_PARAM6, 7 , '*')='*******' then
		:p_where :='and sit.codsg like ''%''';
	else
		if substr(LPAD(:P_PARAM6, 7 , '0'), 4, 4)='****' then
			:p_where := 'and  to_char(sit.codsg,''FM0000000'') like ''' || substr(LPAD(:P_PARAM6, 7 , '0'), 1, 3) ||'%'' ';
		else
			if substr(LPAD(:P_PARAM6, 7 , '0'), 6, 2)='**' then
				:p_where := 'and  to_char(sit.codsg,''FM0000000'') like ''' || substr(LPAD(:P_PARAM6, 7 , '0'), 1, 5) ||'%'' ';
			else
				:p_where := 'and  to_char(sit.codsg,''FM0000000'') = ''' || LPAD(:P_PARAM6, 7 , '0') ||''' ';
			end if;
		end if;
	end if;
  end if;

  if (:P_PARAM5!=0) and (:P_PARAM5 is not null) and (:P_PARAM8 = 'ACH') then
	:p_where := :p_where ||'and  to_char(st.scentrefrais) = ''' || :P_PARAM5 ||''' ';
  end if;
  if (:P_PARAM8 != 'ACH') or (:P_PARAM8 is null) then 
		:P_PARAM5 := NULL;   
  end if;

 --srw.message(1,:P_where);  
  
  return (TRUE);
end;]]>
      </textSource>
    </function>
    <function name="f_cfrais1formattrigger">
      <textSource>
      <![CDATA[function F_CFRAIS1FormatTrigger return boolean is
Text varchar2(60);
begin
  if NVL(LENGTH(:P_PARAM5), 0)!= 0 then 
	 Select LIBCFRAIS into Text from centre_frais where to_char(codcfrais) = :P_PARAM5;
	 Text := :P_PARAM5 || ' - ' ||Text;
	 SRW.SET_FIELD_CHAR(0,Text);
  end if;
  return (TRUE);
end;]]>
      </textSource>
    </function>
