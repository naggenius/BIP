    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
 
      l_where_dpg 	VARCHAR2(1000) := '';
      l_p_code_dpg 	VARCHAR2(10);
      l_perim_me		VARCHAR2(1000) := '';
      code_dpg 			VARCHAR2(10) := '';
      v_annee       budget.annee%type;

BEGIN
	
	-- On vire tous les espaces
	code_dpg := RTRIM(LTRIM(:P_PARAM6));
	
	-- Initialisation de P_WHERE
	l_perim_me := pack_global.lire_perime(:P_GLOBAL);	
	
	:P_WHERE :=' AND l.codsg IN (SELECT codsg FROM vue_dpg_perime where INSTR('''|| l_perim_me ||''', codbddpg) > 0 )'; 

			begin -- Block de Test
  				 -- CAS : pas de données retournées
					 SELECT nvl(b.ANNEE,0) annee into v_annee
					 FROM budget b,ligne_bip l, consomme c, application a
					 WHERE ROWNUM=1
					 AND l.pid = b.pid (+)
           AND l.PID = c.PID (+)
           AND b.annee = c.annee
           AND l.airt = a.airt
           AND ( (a.airt = :P_param7) OR (:P_param7 is null) )
           AND l.codsg IN (SELECT codsg FROM vue_dpg_perime where INSTR(''|| l_perim_me ||'', codbddpg) > 0 )
           AND l.CODSG >= to_number(''|| replace(NVL(code_dpg,'*******'),'*','0') || '')
           AND l.CODSG <= to_number(''|| replace(NVL(code_dpg,'*******'),'*','9') || '');
				  	
		 exception
					when others then v_annee:=0;
	   end; -- Fin du block de Test
	
		 ----srw.message(1, '[ANNEE] : ' || v_annee);
	
		 if (v_annee is null or v_annee=0) then
		  
		  :P_UNION := ' UNION SELECT ''0'',''0'',0,0,0,0,0,0,0 from dual order by 1,2,3 ';
		  ----srw.message(1, '[UNION] : ' || :P_UNION);
		  :P_ORDER := '';				
	   
	   else
	
		  :P_ORDER := 'order by l.AIRT, a.alibel, b.ANNEE';
		  :P_UNION := '';	
	
	   end if;
	-----------------------------------------------------------------------
 	-- Conditions WHERE sur dpg
 	-----------------------------------------------------------------------
 	IF LPAD(code_dpg, 7, '*')='*******' OR code_dpg IS NULL THEN
	  l_where_dpg := '';
	  :P_DPG := 'POUR LE DPG : '|| code_dpg;
 	ELSE
 		:P_DPG := 'POUR LE DPG : '|| code_dpg;
 		----srw.message(1, '[P_DPG] : ' || :P_DPG);
	  l_where_dpg := ' l.CODSG >= to_number('''|| replace(code_dpg,'*','0') || ''')';
	  l_where_dpg := l_where_dpg || ' AND l.CODSG <= to_number('''|| replace(code_dpg,'*','9') || ''')'; 	  
	END IF;
	   
	if ( NVL(LENGTH(l_where_dpg), 0) != 0 ) then 
		:P_WHERE := :P_WHERE || ' AND ( ' || l_where_dpg || ' ) ';
		:P_ORDER := :P_ORDER;
		:P_UNION := :P_UNION;
	end if;
	
	if ( NVL(LENGTH(:P_WHERE), 0) != 0 )
	then
		--srw.message(1, '[P_WHERE] : ' || :P_WHERE);
	end if;
 	 	 	
  RETURN TRUE; 
  	
end;]]>
      </textSource>
    </function>
    <function name="r_annee_appliformattrigger">
      <textSource>
      <![CDATA[function R_ANNEE_APPLIFormatTrigger return boolean is
begin

  -- Généré automatiquement à partir de Report Builder.
  if (mod(:annee,2) = '1')
  then
    srw.set_foreground_fill_color('gray8');
    srw.set_fill_pattern('solid');
  end if;



  return (TRUE);
end;]]>
      </textSource>
    </function>
    <function name="cf_cons_totformula" returnType="number">
      <textSource>
      <![CDATA[function CF_CONS_TOTFormula return Number is
    conso_total NUMBER;
    code_dpg VARCHAR2(10);
    l_perim_me		VARCHAR2(1000) := '';
begin
	
	  code_dpg := NVL(RTRIM(LTRIM(:P_PARAM6)),'*******') ;
	  l_perim_me := pack_global.lire_perime(:P_GLOBAL);
	  
--		--srw.message(1, '[AIRT] : ' || :AIRT);
--		--srw.message(1, '[ANNEE] : ' || :ANNEE);
	
  	select nvl(sum(c.XCUSAG),0)
    into conso_total 
    FROM ligne_bip l, consomme c
    WHERE l.PID = c.PID (+)
    AND l.airt = :AIRT
    AND l.CODSG >= to_number(replace(code_dpg,'*','0'))
	  AND l.CODSG <= to_number(replace(code_dpg,'*','9')) 
    and ( (c.ANNEE = :ANNEE) OR (c.ANNEE <> :ANNEE and :ANNEE=(select max(annee) from consomme where pid=c.pid and annee<:ANNEE) ) )
	  AND l.codsg IN (SELECT codsg FROM vue_dpg_perime where INSTR( l_perim_me , codbddpg) > 0 ); 

--		--srw.message(1, '[conso_total] : ' || conso_total);

	return conso_total;
	
end;]]>
      </textSource>
    </function>
