    <function name="afterpform">
      <textSource>
      <![CDATA[function AfterPForm return boolean is
l_chefsProjets VARCHAR2(4000);--PPM 63485 : modification de taille à 4000
begin
	l_chefsProjets := pack_global.lire_globaldata(:P_GLOBAL).chefprojet;
	--srw.message(1, 'Chefs de projets : ' || l_chefsProjets);
  :P_WHERE := ' AND a.ident in (
		select distinct r.ident
		from situ_ress_full f, ressource r, datdebex d
		where f.ident=r.ident
		and (TRUNC(f.datsitu,''YEAR'') <=TRUNC(d.datdebex,''YEAR'') or f.datsitu is null)
		and (TRUNC(f.datdep,''YEAR'')>=TRUNC(d.datdebex,''YEAR'') or f.datdep is null)
    and f.type_situ=''N''
		and ((INSTR('''||l_chefsProjets||''',TO_CHAR(f.cpident,''FM00000''))>0)
		  or(INSTR('''||l_chefsProjets||''',TO_CHAR(f.ident,''FM00000''))>0))) ';
  
  --srw.message(1,:P_where);  
  return (TRUE);
end;]]>
      </textSource>
    </function>
